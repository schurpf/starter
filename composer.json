{
	"name": "schurpf/starter",
    "description": "WordPress Child Theme based on Genesis and Bootstrap as development starter.",
    "keywords": ["genesis", "starter theme"],
    "homepage": "http://schurpf.com",
    "repositories": [
   		{
    	"type": "composer",
    	"url": "http://wpackagist.org"
    	},
        {
        	"type":"package",
        	"package": {
        		"name": "gudh/ihover",
        		"version":"1.0.0",
        		"source": {
        			"url": "https://github.com/gudh/ihover.git",
        			"type": "git",
        			"reference":"gh-pages"
        		}
        	}
        },
        {
        	"type":"package",
        	"package": {
        		"name": "daneden/animate.css",
        		"version":"3.1.0",
        		"source": {
        			"url": "https://github.com/daneden/animate.css.git",
        			"type": "git",
        			"reference":"master"
        		}
        	}
        },
        {
        	"type":"package",
        	"package": {
        		"name": "dirkgroenen/jquery-viewport-checker",
        		"version":"1.2.0",
        		"source": {
        			"url": "https://github.com/dirkgroenen/jQuery-viewport-checker.git",
        			"type": "git",
        			"reference":"master"
        		}
        	}
        },
        {
        	"type":"package",
        	"package": {
        		"name": "teleject/hisrc",
        		"version":"0.0.0",
        		"source": {
        			"url": "https://github.com/teleject/hisrc.git",
        			"type": "git",
        			"reference":"master"
        		}
        	}
        },
        {
        	"type":"package",
        	"package": {
        		"name": "twittem/wp-bootstrap-navwalker",
        		"version":"2.0.4",
        		"source": {
        			"url": "https://github.com/twittem/wp-bootstrap-navwalker.git",
        			"type": "git",
        			"reference":"master"
        		}
        	}
        },
        {
            "type":"package",
            "package": {
                "name": "leafo/sticky-kit",
                "version":"1.0.4",
                "source": {
                    "url": "https://github.com/leafo/sticky-kit.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],


    "require": {
    	"fancyguy/webroot-installer": "1.*",
    	"twbs/bootstrap-sass": "3.2.*@dev",
    	 "gudh/ihover": "1.0.0",
    	 "daneden/animate.css": "3.1.0",
    	 "dirkgroenen/jquery-viewport-checker": "1.2.0",
    	 "teleject/hisrc": "0.0.0",
    	 "twittem/wp-bootstrap-navwalker": "2.0.4",
         "leafo/sticky-kit": "1.0.4",
    	 "wpackagist/advanced-custom-fields": "*"
    },
    "extra": {
	    "installer-paths": {
	    	"plugins/{$name}/": ["type:wordpress-plugin"]
	    	}
    }
}
